{
    "name": "ariaieboy/caprover-sdk",
    "description": "a PHP SDK for CapRover api",
    "keywords": [
        "ariaieboy",
        "caprover-sdk"
    ],
    "homepage": "https://github.com/ariaieboy/caprover-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "AriaieBOY",
            "email": "smr.seddighy@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20"
    },
    "autoload": {
        "psr-4": {
            "Ariaieboy\\CaproverSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ariaieboy\\CaproverSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
