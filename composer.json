{
    "name": "ariaieboy/caprover-sdk",
    "description": "CapRover SDK for PHP and Laravel",
    "keywords": [
        "ariaieboy",
        "laravel",
        "caprover-laravel",
        "caprover sdk",
        "sdk"
    ],
    "homepage": "https://github.com/ariaieboy/caprover-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "AriaieBOY",
            "email": "ariaieboy.ir@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~8.1.0|~8.2.0",
        "illuminate/contracts": "^9.30|^10.0",
        "spatie/laravel-package-tools": "^1.13.5"
    },
    "require-dev": {
        "graham-campbell/testbench": "^6",
        "pestphp/pest": "^2",
        "pestphp/pest-plugin-laravel": "^2"
    },
    "autoload": {
        "psr-4": {
            "Ariaieboy\\CaproverSDK\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ariaieboy\\CaproverSDK\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ariaieboy\\CaproverSDK\\CaproverLaravelServiceProvider"
            ],
            "aliases": {
                "CaproverLaravel": "Ariaieboy\\CaproverSDK\\Facades\\CaproverLaravel"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
